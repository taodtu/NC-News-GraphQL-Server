{
  "name": "nc_news_graphql_server",
  "version": "1.0.0",
  "description": "",
  "main": "listen.js",
  "scripts": {
    "start": "node server/index.js",
    "setup-dbs": "psql -f db/setup.sql",
    "seed-dev": "npm run setup-dbs && knex seed:run",
    "seed-test": "npm run setup-dbs && NODE_ENV=test knex seed:run",
    "migrate-make": "knex migrate:make",
    "migrate-latest": "knex migrate:latest",
    "migrate-rollback": "knex migrate:rollback",
    "test": "mocha ./server/tests/spec.js",
    "seed": "knex seed:run",
    "dev": "nodemon listen.js",
    "start-app": "node listen.js",
    "seed:prod": "NODE_ENV=production DB_URL=$(heroku config:get DATABASE_URL) knex seed:run",
    "migrate-latest:prod": "NODE_ENV=production DB_URL=$(heroku config:get DATABASE_URL) knex migrate:latest",
    "migrate-rollback:prod": "NODE_ENV=production DB_URL=$(heroku config:get DATABASE_URL) knex migrate:rollback"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "apollo-server": "^2.25.4",
    "apollo-server-express": "^2.6.1",
    "axios": "^0.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "graphql": "^14.3.1",
    "graphql-iso-date": "^3.6.1",
    "knex": "^0.17.3",
    "pg": "^7.11.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "chai": "^4.2.0",
    "mocha": "^6.1.4",
    "nodemon": "^1.19.1",
    "supertest": "^4.0.2"
  }
}